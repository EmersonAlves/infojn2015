inicio
    variavel texto aluno [ 11 ]
    inteiro x
    x <- 1
    enquanto ( x <= 10 ) faz
        escrever "digite o nome do aluno" , x , "\n"
        ler aluno [ x ]
        x <- x + 1
    fimenquanto
    x <- 1
    enquanto x <= 10 faz
        escrever aluno [ x ] + "\n"
        x <- x + 1 
    fimenquanto
fim

