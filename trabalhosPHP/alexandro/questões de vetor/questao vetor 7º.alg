inicio
    variavel inteiro n [ 11 ] , p [ 11 ] , nn [ 11 ] , x,q
    x <- 1
    enquanto x <= 10 faz
        escrever " \n digite o numero \n "
        ler n [ x ]
        se n [ x ] >= 0 entao
            escrever " numeros positivos "
        senao
            escrever " numeros negativos "
        fimse
        x <- x + 1
    fimenquanto
    
fim

