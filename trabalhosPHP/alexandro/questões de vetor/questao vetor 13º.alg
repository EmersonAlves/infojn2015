inicio
    variavel texto alunos [ 9 ] ,
    real nota [ 9 ] , media,
    inteiro x , nota [ 9 ] , media
    x <- 1
    enquanto ( x <= 8 ) faz
        escrever " \n digite o nome do aluno  ", x ,":"
        ler alunos [ x ]
        escrever "\n digite a nota do aluno  ", x ,":"
        ler nota [ x ]
        x <- x + 1
   fimenquanto   
fim

